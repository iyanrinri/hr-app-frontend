T,YAAqD,MAAA,EAAjD,OAAM,YAAA,mBAAe,GAAG,YAAY,IAAG,SAAK,CAAA;AAAA,4BAChDA,YAIK,MAAA,EAJD,OAAM,+CAA2C;AAAA,8BAClDA,YAAmD,aAA9C,iBAAYS,gBAAG,GAAG,kBAAkB,IAAG,KAAC,CAAA;AAAA,8BAC7CT,YAA8C,aAAzC,aAAQS,gBAAG,GAAG,iBAAiB,IAAG,KAAC,CAAA;AAAA,8BAC7B,GAAG,+BAAdH,YAA+F,OAAA;AAAA;gCAAjE,OAAM;AAAA,8BAAA,GAAiB,gBAAWG,gBAAG,GAAG,eAAe,IAAG,KAAC,CAAA;;4BAE5FT,YAQK,MAAA,EARD,OAAM,0BAAsB;AAAA,8BAC7BA,YAMW,qBAAA;AAAA,gCALT,SAAQ;AAAA,gCACR,OAAM;AAAA,gCACL,SAAK,CAAA,WAAE,iBAAiB,GAAG,EAAE;AAAA,8BAAA;iDAE9B,MAAyB;AAAA,kCAAzBA,YAAyBP,MAAA,KAAA,GAAA,EAAlB,OAAM,WAAS;AAAA,gCAAA;;;;;;;;;;;;;;;;;;UAWzC,QAAQ,gBAAA;AAAA,UACR,eAAe,SAAA;AAAA,UACf,qBAAO,gBAAA,QAAe;AAAA,UACtB,WAAS;AAAA,QAAA;;;;;;;;;;;;;","names":["_ssrRenderAttrs","_mergeProps","_ssrRenderComponent","_unref","_ssrIncludeBooleanAttr","_ssrLooseContain","_ssrInterpolate","$emit","_push","_parent","_createVNode","_","_scopeId","_vModelCheckbox","_vModelText","_ssrRenderList","_createBlock","_Fragment","_renderList","_toDisplayString"],"sources":["../../../../components/leaves/LeaveTypeModal.vue","../../../../pages/[tenant_slug]/dashboard/leaves/periods/[id]/index.vue"],"sourcesContent":["<script setup lang=\"ts\">\nimport { ref } from 'vue'\nimport { useCreateLeaveType } from '@/composables/useLeaves'\nimport { X } from 'lucide-vue-next'\nimport type { CreateLeaveTypePayload } from '@/types/leaves'\n\nconst props = defineProps<{\n  isOpen: boolean\n  leavePeriodId: number\n}>()\n\nconst emit = defineEmits<{\n  (e: 'close'): void\n  (e: 'created'): void // To trigger refresh in parent\n}>()\n\nconst { mutate: createType, loading: isPending } = useCreateLeaveType()\n\nconst form = ref<Omit<CreateLeaveTypePayload, 'leavePeriodId'>>({\n    type: '',\n    name: '',\n    description: '',\n    defaultQuota: 12,\n    maxConsecutiveDays: 5,\n    advanceNoticeDays: 3,\n    isCarryForward: false,\n    maxCarryForward: 0,\n    isActive: true\n})\n\nconst errors = ref<Partial<Record<keyof CreateLeaveTypePayload, string>>>({})\n\nconst validate = () => {\n    errors.value = {}\n    if (!form.value.type || form.value.type.length < 2) errors.value.type = 'Code required (e.g. ANNUAL)'\n    if (!form.value.name || form.value.name.length < 2) errors.value.name = 'Name required'\n    return Object.keys(errors.value).length === 0\n}\n\nconst onSubmit = async () => {\n    if (!validate()) return\n\n    try {\n        await createType({ ...form.value, leavePeriodId: props.leavePeriodId })\n        emit('created')\n        emit('close')\n    } catch (e) {\n        // error handling\n    }\n}\n</script>\n\n<template>\n    <div v-if=\"isOpen\" class=\"fixed inset-0 z-50 flex items-center justify-center bg-black/50 p-4\">\n       <div class=\"bg-white rounded-lg shadow-xl w-full max-w-lg max-h-[90vh] overflow-y-auto\">\n         <div class=\"p-6 border-b flex justify-between items-center\">\n           <h3 class=\"text-lg font-bold\">Add Leave Type</h3>\n           <button @click=\"$emit('close')\" class=\"text-gray-500 hover:text-gray-700\">\n             <X class=\"w-5 h-5\" />\n           </button>\n         </div>\n         <div class=\"p-6\">\n           <form @submit.prevent=\"onSubmit\" class=\"space-y-4\">\n              <div class=\"grid grid-cols-2 gap-4\">\n                <UiInput label=\"Type Code (e.g. ANNUAL)\" v-model=\"form.type\" :error=\"errors.type\" />\n                <UiInput label=\"Display Name\" v-model=\"form.name\" :error=\"errors.name\" />\n              </div>\n              \n              <div class=\"grid grid-cols-3 gap-4\">\n                 <UiInput label=\"Default Quota\" type=\"number\" v-model.number=\"form.defaultQuota\" />\n                 <UiInput label=\"Max Consec.\" type=\"number\" v-model.number=\"form.maxConsecutiveDays\" />\n                 <UiInput label=\"Adv. Notice\" type=\"number\" v-model.number=\"form.advanceNoticeDays\" />\n              </div>\n\n              <div class=\"space-y-2 p-3 bg-gray-50 rounded border\">\n                <label class=\"flex items-center gap-2\">\n                  <input type=\"checkbox\" v-model=\"form.isCarryForward\" class=\"rounded border-gray-300\" />\n                  <span class=\"text-sm font-medium\">Allow Carry Forward?</span>\n                </label>\n                <div v-if=\"form.isCarryForward\">\n                    <UiInput label=\"Max Carry Fwd Days\" type=\"number\" v-model.number=\"form.maxCarryForward\" />\n                </div>\n              </div>\n\n              <div class=\"space-y-1\">\n                <label class=\"text-sm font-medium\">Description</label>\n                <textarea class=\"w-full border rounded p-2 text-sm\" v-model=\"form.description\" />\n              </div>\n\n              <div class=\"flex justify-end gap-3 pt-4\">\n                <UiButton type=\"button\" variant=\"ghost\" @click=\"$emit('close')\">Cancel</UiButton>\n                <UiButton type=\"submit\" :disabled=\"isPending\">Create Type</UiButton>\n              </div>\n           </form>\n         </div>\n       </div>\n    </div>\n</template>\n","<script setup lang=\"ts\">\nimport { ref, computed, watchEffect } from 'vue'\nimport { useRoute, useRouter } from 'vue-router'\nimport { \n    useLeavePeriod, \n    useUpdateLeavePeriod, \n    useLeaveTypes, \n    useDeleteLeaveType \n} from '@/composables/useLeaves'\nimport { ArrowLeft, Plus, Trash, Settings } from 'lucide-vue-next'\nimport LeaveTypeModal from '@/components/leaves/LeaveTypeModal.vue'\nimport type { UpdateLeavePeriodPayload } from '@/types/leaves'\n\nconst route = useRoute()\nconst router = useRouter()\nconst periodId = computed(() => Number(route.params.id))\nconst tenantSlug = route.params.tenant_slug as string\n\nconst { data: period, loading: isLoadingPeriod, refresh: refreshPeriod, error } = useLeavePeriod(periodId)\nconst { mutate: updatePeriod, loading: isUpdating } = useUpdateLeavePeriod()\n\nconst { data: leaveTypes, loading: isLoadingTypes, refresh: refreshTypes } = useLeaveTypes(periodId)\nconst { mutate: deleteType } = useDeleteLeaveType()\n\nconst form = ref<UpdateLeavePeriodPayload>({\n    name: '',\n    startDate: '',\n    endDate: '',\n    description: '',\n    isActive: false\n})\n\nconst isTypeModalOpen = ref(false)\n\nwatchEffect(() => {\n    if (period.value) {\n        form.value = {\n            name: period.value.name,\n            startDate: period.value.startDate.split('T')[0],\n            endDate: period.value.endDate.split('T')[0],\n            description: period.value.description,\n            isActive: period.value.isActive\n        }\n    }\n})\n\nconst onPeriodSubmit = async () => {\n    await updatePeriod(periodId.value, form.value)\n    refreshPeriod()\n}\n\nconst handleDeleteType = async (typeId: number) => {\n    if (confirm('Delete this leave type?')) {\n        await deleteType(typeId)\n        refreshTypes()\n    }\n}\n\nconst onTypeCreated = () => {\n    refreshTypes()\n}\n\ndefinePageMeta({\n  layout: 'dashboard'\n})\n</script>\n\n<template>\n    <div class=\"space-y-8\">\n      <div v-if=\"isLoadingPeriod\" class=\"p-8 text-center\">Loading...</div>\n      <div v-else-if=\"error || !period\" class=\"p-8 text-center text-red-500\">Period not found</div>\n      <template v-else>\n          <!-- Header -->\n          <div class=\"flex items-center gap-4\">\n            <UiButton variant=\"ghost\" @click=\"router.back()\">\n              <ArrowLeft class=\"w-4 h-4 mr-2\" /> Back\n            </UiButton>\n            <h2 class=\"text-2xl font-bold text-gray-900\">Edit Leave Period</h2>\n          </div>\n\n          <!-- Period Details Form -->\n          <UiCard>\n            <UiCardHeader><UiCardTitle>Configuration</UiCardTitle></UiCardHeader>\n            <UiCardContent>\n              <form @submit.prevent=\"onPeriodSubmit\" class=\"space-y-6\">\n                <div class=\"grid grid-cols-1 md:grid-cols-2 gap-6\">\n                   <UiInput \n                     label=\"Period Name\"\n                     v-model=\"form.name\"\n                   />\n                   <div class=\"flex items-end pb-2\">\n                     <label class=\"flex items-center gap-2 cursor-pointer\">\n                       <input type=\"checkbox\" class=\"w-5 h-5 rounded border-gray-300 text-brand-navy focus:ring-brand-navy\" v-model=\"form.isActive\" />\n                       <span class=\"text-sm font-medium text-gray-700\">Set as Active Period</span>\n                     </label>\n                   </div>\n                   \n                   <UiInput \n                     label=\"Start Date\" \n                     type=\"date\"\n                     v-model=\"form.startDate\"\n                   />\n                   <UiInput \n                     label=\"End Date\" \n                     type=\"date\"\n                     v-model=\"form.endDate\"\n                   />\n                </div>\n                \n                <div class=\"space-y-1\">\n                  <label class=\"text-sm font-medium text-gray-700\">Description</label>\n                  <textarea \n                    class=\"w-full min-h-[80px] border border-gray-300 rounded-md p-2\"\n                    v-model=\"form.description\"\n                  />\n                </div>\n\n                <div class=\"flex justify-end\">\n                  <UiButton type=\"submit\" :disabled=\"isUpdating\">Save Changes</UiButton>\n                </div>\n              </form>\n            </UiCardContent>\n          </UiCard>\n\n          <!-- Leave Types Section -->\n          <div class=\"space-y-4\">\n            <div class=\"flex justify-between items-center\">\n               <h3 class=\"text-xl font-bold text-gray-800\">Leave Types & Quotas</h3>\n               <UiButton @click=\"isTypeModalOpen = true\">\n                 <Plus class=\"w-4 h-4 mr-2\" /> Add Leave Type\n               </UiButton>\n            </div>\n\n            <UiCard>\n              <UiCardContent class=\"p-0 overflow-hidden\">\n                 <table class=\"w-full text-sm text-left\">\n                   <thead class=\"bg-gray-50 border-b\">\n                     <tr>\n                       <th class=\"px-4 py-3 font-medium text-gray-500\">Code</th>\n                       <th class=\"px-4 py-3 font-medium text-gray-500\">Name</th>\n                       <th class=\"px-4 py-3 font-medium text-gray-500\">Quota</th>\n                       <th class=\"px-4 py-3 font-medium text-gray-500\">Settings</th>\n                       <th class=\"px-4 py-3 font-medium text-right text-gray-500\">Action</th>\n                     </tr>\n                   </thead>\n                   <tbody class=\"divide-y\">\n                     <tr v-if=\"isLoadingTypes\">\n                         <td colspan=\"5\" class=\"p-4 text-center\">Loading types...</td>\n                     </tr>\n                     <tr v-else-if=\"!leaveTypes || leaveTypes.length === 0\">\n                         <td colspan=\"5\" class=\"p-4 text-center text-gray-500 italic\">No leave types defined.</td>\n                     </tr>\n                     <tr v-else v-for=\"lt in leaveTypes\" :key=\"lt.id\" class=\"hover:bg-gray-50\">\n                           <td class=\"px-4 py-3 font-mono text-xs\">{{ lt.type }}</td>\n                           <td class=\"px-4 py-3 font-medium\">{{ lt.name }}</td>\n                           <td class=\"px-4 py-3\">{{ lt.defaultQuota }} Days</td>\n                           <td class=\"px-4 py-3 text-xs text-gray-500 space-y-1\">\n                              <div>Max Consec: {{ lt.maxConsecutiveDays }}d</div>\n                              <div>Notice: {{ lt.advanceNoticeDays }}d</div>\n                              <div v-if=\"lt.isCarryForward\" class=\"text-green-600\">Carry Fwd: {{ lt.maxCarryForward }}d</div>\n                           </td>\n                           <td class=\"px-4 py-3 text-right\">\n                              <UiButton \n                                variant=\"destructive\" \n                                class=\"h-8 px-2\"\n                                @click=\"handleDeleteType(lt.id)\"\n                              >\n                                <Trash class=\"w-4 h-4\" />\n                              </UiButton>\n                           </td>\n                     </tr>\n                   </tbody>\n                 </table>\n              </UiCardContent>\n            </UiCard>\n          </div>\n\n          <LeaveTypeModal \n            :isOpen=\"isTypeModalOpen\"\n            :leavePeriodId=\"periodId\"\n            @close=\"isTypeModalOpen = false\"\n            @created=\"onTypeCreated\"\n          />\n      </template>\n    </div>\n</template>\n"],"version":3}