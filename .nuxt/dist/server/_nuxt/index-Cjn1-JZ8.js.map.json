0BAGvFA,YAiBQ,SAAA,EAjBD,OAAM,uCAAmC;AAAA,6BAC5CQ,UAAA,IAAA,GAAAD,YAeKE,UAAA,MAAAC,WAfkBb,MAAA,SAAA,EAAU,kBAAtB,aAAQ;kDAAnBU,YAeK,MAAA;AAAA,gCAf8C,KAAK,SAAS;AAAA,gCAAI,OAAM;AAAA,8BAAA;gCACxEP,YAOK,MAAA,EAPD,OAAM,iCAA6B;AAAA,kCACnCA,YAKM,OAAA,MAAA;AAAA,oCAJJA,YAEI,KAAA,EAFD,OAAM,uCAAmCK,gBACvC,SAAS,SAAS,IAAG,MAACA,gBAAG,SAAS,QAAQ,GAAA,CAAA;AAAA,oCAE/CL,YAAyD,OAAtD,OAAM,wBAAA,GAAuBK,gBAAI,SAAS,KAAK,GAAA,CAAA;AAAA,kCAAA;;gCAGxDL,YAEK,QAFD,OAAM,oDAAA,GAAmDK,gBACtD,SAAS,UAAU,GAAA,CAAA;AAAA,gCAE1BL,YAEK,QAFD,OAAM,oDAAA,GAAmDK,gBACtD,SAAS,QAAQ,GAAA,CAAA;AAAA,8BAAA;;;;0CAMrCE,YAAkF,KAAA;AAAA;wBAAxE,OAAM;AAAA,sBAAA,GAAiC,+BAA6B;AAAA,oBAAA;AAAA;;;;;;gBApCjFP,YAKe,yBAAA,MAAA;AAAA,mCAJb,MAGc;AAAA,oBAHdA,YAGc,wBAAA,EAHD,OAAM,uBAAmB;AAAA,uCACpC,MAA2C;AAAA,wBAA3CA,YAA2CH,MAAA,KAAA,GAAA,EAApC,OAAM,6BAA2B;AAAA,wBAAGS,gBAAA,wCACtBT,MAAA,SAAA,EAAU,gBAAgB,MAAM,IAAG,MAC1D,CAAA;AAAA,sBAAA;;;;;;gBAEFG,YA+BgB,0BAAA,MAAA;AAAA,mCA9Bb,MA4BM;AAAA,oBA5BKH,MAAA,SAAA,EAAU,gBAAgB,SAAM,kBAA3CU,YA4BM,OAAA;AAAA;sBA5B2C,OAAM;AAAA,oBAAA;sBACrDP,YA0BQ,SAAA,EA1BD,OAAM,yCAAqC;AAAA,wBAC/CA,YAMQ,SAAA,EAND,OAAM,gBAAY;AAAA,0BACvBA,YAIK,MAAA,MAAA;AAAA,4BAHHA,YAAyF,MAAA,EAArF,OAAM,kEAAA,GAAkE,UAAQ;AAAA,4BACpFA,YAA2F,MAAA,EAAvF,OAAM,kEAAA,GAAkE,YAAU;AAAA,4BACtFA,YAAyF,MAAA,EAArF,OAAM,kEAAA,GAAkE,UAAQ;AAAA,0BAAA;;wBAGvFA,YAiBQ,SAAA,EAjBD,OAAM,uCAAmC;AAAA,2BAC5CQ,UAAA,IAAA,GAAAD,YAeKE,UAAA,MAAAC,WAfkBb,MAAA,SAAA,EAAU,kBAAtB,aAAQ;gDAAnBU,YAeK,MAAA;AAAA,8BAf8C,KAAK,SAAS;AAAA,8BAAI,OAAM;AAAA,4BAAA;8BACxEP,YAOK,MAAA,EAPD,OAAM,iCAA6B;AAAA,gCACnCA,YAKM,OAAA,MAAA;AAAA,kCAJJA,YAEI,KAAA,EAFD,OAAM,uCAAmCK,gBACvC,SAAS,SAAS,IAAG,MAACA,gBAAG,SAAS,QAAQ,GAAA,CAAA;AAAA,kCAE/CL,YAAyD,OAAtD,OAAM,wBAAA,GAAuBK,gBAAI,SAAS,KAAK,GAAA,CAAA;AAAA,gCAAA;;8BAGxDL,YAEK,QAFD,OAAM,oDAAA,GAAmDK,gBACtD,SAAS,UAAU,GAAA,CAAA;AAAA,8BAE1BL,YAEK,QAFD,OAAM,oDAAA,GAAmDK,gBACtD,SAAS,QAAQ,GAAA,CAAA;AAAA,4BAAA;;;;wCAMrCE,YAAkF,KAAA;AAAA;sBAAxE,OAAM;AAAA,oBAAA,GAAiC,+BAA6B;AAAA,kBAAA;;;;;;;;;;;;;;;;;;;;","names":["_ssrRenderAttrs","_mergeProps","_ssrRenderComponent","_unref","_push","_parent","_createVNode","_ssrRenderClass","_ssrInterpolate","_","_scopeId","_toDisplayString","_createTextVNode","_createBlock","_openBlock","_Fragment","_renderList"],"sources":["../../../../components/notifications/NotificationPermissionButton.vue","../../../../pages/[tenant_slug]/dashboard/attendance/today/index.vue"],"sourcesContent":["<script setup lang=\"ts\">\nimport { ref, onMounted } from 'vue'\nimport { Bell, BellOff } from 'lucide-vue-next'\n\nconst permission = ref<NotificationPermission>('default')\n\nonMounted(() => {\n  if ('Notification' in window) {\n    permission.value = Notification.permission\n  }\n})\n\nconst requestPermission = async () => {\n  if ('Notification' in window) {\n    const result = await Notification.requestPermission()\n    permission.value = result\n    \n    if (result === 'granted') {\n       new Notification('Notifications Enabled! ðŸŽ‰', {\n          body: 'You will now receive attendance updates',\n          icon: '/favicon.ico',\n        })\n    }\n  }\n}\n</script>\n\n<template>\n  <div v-if=\"permission === 'granted'\" class=\"flex items-center gap-2 text-sm text-green-600\">\n    <Bell class=\"w-4 h-4\" />\n    <span>Notifications enabled</span>\n  </div>\n\n  <div v-else-if=\"permission === 'denied'\" class=\"flex items-center gap-2 text-sm text-red-600\">\n    <BellOff class=\"w-4 h-4\" />\n    <span>Notifications blocked</span>\n  </div>\n\n  <UiButton v-else @click=\"requestPermission\" variant=\"secondary\" class=\"text-sm\">\n    <Bell class=\"w-4 h-4 mr-2\" />\n    Enable Notifications\n  </UiButton>\n</template>\n","<script setup lang=\"ts\">\nimport { ref, watch, onMounted } from 'vue'\nimport { \n  Users, \n  UserCheck, \n  UserX, \n  Clock, \n  Calendar,\n  RefreshCw \n} from 'lucide-vue-next'\nimport { useTodayAttendanceDashboard } from '@/composables/useAttendance'\nimport { useNotificationStore } from '@/stores/notifications'\nimport NotificationPermissionButton from '@/components/notifications/NotificationPermissionButton.vue'\n\ndefinePageMeta({\n  layout: 'dashboard'\n})\n\nconst route = useRoute()\nconst tenantSlug = route.params.tenant_slug as string\nconst notificationStore = useNotificationStore()\n\nconst { data: dashboard, loading, refresh } = useTodayAttendanceDashboard()\n\n// Auto-refresh when dashboard update trigger changes (from WebSocket)\nwatch(() => notificationStore.dashboardUpdateTrigger, (newVal) => {\n    if (newVal > 0) {\n        console.log('ðŸ”„ [AUTO-REFRESH] Dashboard refreshing due to dashboard-update event')\n        refresh()\n    }\n})\n\n// Also refresh on notifications?\nwatch(() => notificationStore.notifications, (newVal) => {\n    if (newVal.length > 0) {\n        // Debounce?\n        refresh()\n    }\n}, { deep: true })\n\nconst formatTime = (dateObj: string | number | Date | null | undefined) => {\n    if (!dateObj) return '-'\n    try {\n      const date = new Date(dateObj)\n      return date.toLocaleTimeString('en-US', { \n        hour: '2-digit', \n        minute: '2-digit',\n        hour12: false\n      })\n    } catch {\n      return '-'\n    }\n}\n\nconst getStatusBadge = (status: string | undefined) => {\n    const badges: Record<string, string> = {\n      PRESENT: 'bg-green-100 text-green-800',\n      LATE: 'bg-yellow-100 text-yellow-800',\n      ABSENT: 'bg-red-100 text-red-800',\n      EXCUSED: 'bg-blue-100 text-blue-800',\n    }\n    return badges[status || ''] || 'bg-gray-100 text-gray-800'\n}\n\nconst formatDate = (dateString: string) => {\n    return new Date(dateString).toLocaleDateString('en-US', { \n        weekday: 'long', \n        year: 'numeric', \n        month: 'long', \n        day: 'numeric' \n    })\n}\n</script>\n\n<template>\n    <div class=\"space-y-6\">\n      <div v-if=\"loading && !dashboard\" class=\"flex flex-col items-center justify-center py-12\">\n         <div class=\"animate-spin rounded-full h-12 w-12 border-b-2 border-brand-navy mb-4\"></div>\n         <p class=\"text-gray-600\">Loading today's attendance...</p>\n      </div>\n\n      <div v-else-if=\"!dashboard\" class=\"text-center py-12 text-gray-500\">\n        No attendance data available for today\n      </div>\n\n      <div v-else class=\"space-y-6\">\n          <!-- Header -->\n          <div class=\"flex justify-between items-center\">\n            <div>\n              <h2 class=\"text-2xl font-bold text-gray-900\">Today's Attendance Dashboard</h2>\n              <div class=\"flex items-center gap-4 mt-2\">\n                <div class=\"flex items-center text-sm text-gray-600\">\n                  <Calendar class=\"w-4 h-4 mr-2\" />\n                  {{ formatDate(dashboard.date) }}\n                </div>\n                <div class=\"flex items-center text-sm\">\n                  <div class=\"w-2 h-2 rounded-full mr-2\" :class=\"notificationStore.isConnected ? 'bg-green-500' : 'bg-red-500'\"></div>\n                  {{ notificationStore.isConnected ? 'Live Updates Active' : 'Disconnected' }}\n                </div>\n                <NotificationPermissionButton />\n              </div>\n            </div>\n            <div class=\"flex gap-2\">\n              <UiButton @click=\"refresh\" variant=\"secondary\">\n                <RefreshCw class=\"w-4 h-4 mr-2\" />\n                Refresh\n              </UiButton>\n              <NuxtLink :to=\"tenantSlug ? `/${tenantSlug}/dashboard/attendance/history` : '/dashboard/attendance/history'\">\n                 <UiButton variant=\"secondary\">View History</UiButton>\n              </NuxtLink>\n            </div>\n          </div>\n\n          <!-- Summary Stats -->\n          <div class=\"grid grid-cols-1 md:grid-cols-4 gap-4\">\n            <UiCard>\n              <UiCardContent class=\"pt-6\">\n                <div class=\"flex items-center justify-between\">\n                  <div>\n                    <p class=\"text-sm text-gray-600 mb-1\">Total Employees</p>\n                    <p class=\"text-3xl font-bold text-gray-900\">{{ dashboard.summary.totalEmployees }}</p>\n                  </div>\n                  <Users class=\"w-10 h-10 text-gray-400\" />\n                </div>\n              </UiCardContent>\n            </UiCard>\n\n            <UiCard>\n              <UiCardContent class=\"pt-6\">\n                <div class=\"flex items-center justify-between\">\n                  <div>\n                    <p class=\"text-sm text-gray-600 mb-1\">Present</p>\n                    <p class=\"text-3xl font-bold text-green-600\">{{ dashboard.summary.totalPresent }}</p>\n                    <p class=\"text-xs text-gray-500 mt-1\">{{ dashboard.summary.attendanceRate.toFixed(0) }}% rate</p>\n                  </div>\n                  <UserCheck class=\"w-10 h-10 text-green-400\" />\n                </div>\n              </UiCardContent>\n            </UiCard>\n\n            <UiCard>\n              <UiCardContent class=\"pt-6\">\n                <div class=\"flex items-center justify-between\">\n                  <div>\n                    <p class=\"text-sm text-gray-600 mb-1\">Absent</p>\n                    <p class=\"text-3xl font-bold text-red-600\">{{ dashboard.summary.totalAbsent }}</p>\n                  </div>\n                  <UserX class=\"w-10 h-10 text-red-400\" />\n                </div>\n              </UiCardContent>\n            </UiCard>\n\n            <UiCard>\n              <UiCardContent class=\"pt-6\">\n                <div class=\"flex items-center justify-between\">\n                  <div>\n                    <p class=\"text-sm text-gray-600 mb-1\">Late</p>\n                    <p class=\"text-3xl font-bold text-yellow-600\">{{ dashboard.summary.totalLate }}</p>\n                    <p class=\"text-xs text-gray-500 mt-1\">{{ dashboard.summary.lateRate.toFixed(0) }}% rate</p>\n                  </div>\n                  <Clock class=\"w-10 h-10 text-yellow-400\" />\n                </div>\n              </UiCardContent>\n            </UiCard>\n          </div>\n\n          <!-- Period Info -->\n           <UiCard>\n            <UiCardContent class=\"pt-6\">\n              <div class=\"flex items-center justify-between\">\n                <div>\n                  <p class=\"text-sm text-gray-600\">Attendance Period</p>\n                  <p class=\"text-lg font-bold text-gray-900\">{{ dashboard.attendancePeriod.name }}</p>\n                </div>\n                <div class=\"text-right\">\n                  <p class=\"text-sm text-gray-600\">Working Hours</p>\n                  <p class=\"text-lg font-bold text-gray-900\">\n                    {{ dashboard.attendancePeriod.workingStartTime }} - {{ dashboard.attendancePeriod.workingEndTime }}\n                  </p>\n                  <p class=\"text-xs text-gray-500 mt-1\">\n                    Tolerance: {{ dashboard.attendancePeriod.toleranceMinutes }} minutes\n                  </p>\n                </div>\n              </div>\n            </UiCardContent>\n          </UiCard>\n\n          <!-- Present Employees -->\n          <UiCard>\n            <UiCardHeader>\n              <UiCardTitle class=\"flex items-center\">\n                <UserCheck class=\"w-5 h-5 mr-2 text-green-600\" />\n                Present Employees ({{ dashboard.presentEmployees.length }})\n              </UiCardTitle>\n            </UiCardHeader>\n            <UiCardContent>\n              <div v-if=\"dashboard.presentEmployees.length > 0\" class=\"overflow-x-auto\">\n                <table class=\"min-w-full divide-y divide-gray-200\">\n                    <thead class=\"bg-gray-50\">\n                      <tr>\n                        <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase\">Employee</th>\n                        <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase\">Department</th>\n                        <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase\">Position</th>\n                        <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase\">Check In</th>\n                        <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase\">Check Out</th>\n                        <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase\">Duration</th>\n                        <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase\">Status</th>\n                      </tr>\n                    </thead>\n                    <tbody class=\"bg-white divide-y divide-gray-200\">\n                        <tr v-for=\"employee in dashboard.presentEmployees\" :key=\"employee.id\" class=\"hover:bg-gray-50\">\n                            <td class=\"px-6 py-4 whitespace-nowrap\">\n                                <div>\n                                  <p class=\"text-sm font-medium text-gray-900\">\n                                    {{ employee.firstName }} {{ employee.lastName }}\n                                  </p>\n                                  <p class=\"text-sm text-gray-500\">{{ employee.email }}</p>\n                                </div>\n                            </td>\n                            <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-900\">\n                                {{ employee.department }}\n                            </td>\n                            <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-900\">\n                                {{ employee.position }}\n                            </td>\n                            <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-900\">\n                                {{ formatTime(employee.checkIn) }}\n                            </td>\n                            <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-900\">\n                                {{ formatTime(employee.checkOut) }}\n                            </td>\n                            <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-900\">\n                                {{ employee.workDuration ? `${(employee.workDuration / 60).toFixed(1)}h` : '-' }}\n                            </td>\n                            <td class=\"px-6 py-4 whitespace-nowrap\">\n                                <div class=\"flex flex-col gap-1\">\n                                  <span class=\"inline-flex items-center px-3 py-1 rounded-full text-xs font-medium\" :class=\"getStatusBadge(employee.status)\">\n                                    {{ employee.status }}\n                                  </span>\n                                  <span v-if=\"employee.isLate\" class=\"text-xs text-yellow-600\">\n                                      Late by {{ employee.minutesLate }} min\n                                  </span>\n                                </div>\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n              </div>\n              <p v-else class=\"text-center text-gray-500 py-4\">No employees present yet</p>\n            </UiCardContent>\n          </UiCard>\n\n           <!-- Absent Employees -->\n          <UiCard>\n            <UiCardHeader>\n              <UiCardTitle class=\"flex items-center\">\n                <UserX class=\"w-5 h-5 mr-2 text-red-600\" />\n                Absent Employees ({{ dashboard.absentEmployees.length }})\n              </UiCardTitle>\n            </UiCardHeader>\n            <UiCardContent>\n               <div v-if=\"dashboard.absentEmployees.length > 0\" class=\"overflow-x-auto\">\n                 <table class=\"min-w-full divide-y divide-gray-200\">\n                    <thead class=\"bg-gray-50\">\n                      <tr>\n                        <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase\">Employee</th>\n                        <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase\">Department</th>\n                        <th class=\"px-6 py-3 text-left text-xs font-medium text-gray-500 uppercase\">Position</th>\n                      </tr>\n                    </thead>\n                     <tbody class=\"bg-white divide-y divide-gray-200\">\n                         <tr v-for=\"employee in dashboard.absentEmployees\" :key=\"employee.id\" class=\"hover:bg-gray-50\">\n                            <td class=\"px-6 py-4 whitespace-nowrap\">\n                                <div>\n                                  <p class=\"text-sm font-medium text-gray-900\">\n                                    {{ employee.firstName }} {{ employee.lastName }}\n                                  </p>\n                                  <p class=\"text-sm text-gray-500\">{{ employee.email }}</p>\n                                </div>\n                            </td>\n                            <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-900\">\n                                {{ employee.department }}\n                            </td>\n                            <td class=\"px-6 py-4 whitespace-nowrap text-sm text-gray-900\">\n                                {{ employee.position }}\n                            </td>\n                         </tr>\n                     </tbody>\n                 </table>\n               </div>\n               <p v-else class=\"text-center text-gray-500 py-4\">All employees are present! ðŸŽ‰</p>\n            </UiCardContent>\n          </UiCard>\n      </div>\n    </div>\n</template>\n"],"version":3}